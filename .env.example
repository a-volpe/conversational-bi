# Neon PostgreSQL Connection
DATABASE_URL=postgresql://user:password@ep-xxxxx.region.aws.neon.tech/dbname?sslmode=require

# OpenAI API
OPENAI_API_KEY=sk-xxxxx
OPENAI_MODEL=gpt-4.1-mini

# Data Agent URLs (for FE agent A2A discovery)
CUSTOMERS_AGENT_URL=http://localhost:8001
ORDERS_AGENT_URL=http://localhost:8002
PRODUCTS_AGENT_URL=http://localhost:8003

# Application
STREAMLIT_PORT=8501
LOG_LEVEL=INFO
DEBUG=false

# LangSmith Monitoring (optional - set LANGCHAIN_TRACING_V2=true to enable)
LANGCHAIN_TRACING_V2=true
LANGSMITH_ENDPOINT=https://eu.api.smith.langchain.com
LANGCHAIN_API_KEY=lsv2_pt_xxxxx
LANGCHAIN_PROJECT=conversational-bi

# Seed Data (optional)
SEED_CUSTOMERS=100
SEED_ORDERS=500
